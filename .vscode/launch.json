  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal"
    },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Backend",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}\\api\\server.js"
    },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Frontend",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}\\client\\server.js"
    },
  ]
}
